let 안녕 = "하세요";
let hi = "hello";
let HI = "HELLO";

console.log(안녕); // 하세요 출력
console.log(hi); // hello 출력
console.log(HI); // HELLO 출력

// single-line comments (단일주석)
// 1213213123213123
console.log("hello, world!"); // 이것은 단일행 주석입니다. 

// console.log("hello, world!"); 출력이 안됨

/* multi-line comments
어떤 문자 와도 
개행이 되어도 주석처리가 된다.
주석으로 인정이 됩니다. 
 */
console.log("hello, world!");
