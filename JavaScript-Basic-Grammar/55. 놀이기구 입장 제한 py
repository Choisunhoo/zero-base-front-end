# 놀이기구 입장 제한

from numpy import true_divide


n = [ 
  { "name": "john", "age": 27, "height" : 181 },
  { "name": "alice", "age": 12, "height": 148 },
  { "name": "bob", "age": 14, "height": 156 },
];


for i in range(len(n)):
    x = n[i]['height']
    if x >= 150:
        result = "true"
    else:
        result = "false"
    print(result)
    
