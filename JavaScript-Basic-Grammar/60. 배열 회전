// 배열 회전

/* user code */
function answer(user) {
  let reverse = []; 

  for (let i = user.length - 1; i >= 0; i--) {
    reverse.push(user[i]);
  }

  return reverse;
}
/* main code*/
let input = [ 
  [1, 2, 3, 4], [-1, 6, "hello", -15], ["apple", "banana", "mango"],
];

for (let i = 0; i < input.length; i++) {
  process.stdout.write(`#${i + 1} `);
  console.log(answer(input[i]));
}  
